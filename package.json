{
  "name": "tab-group-automator",
  "version": "3.7.1",
  "description": "Extensão WebExtension que organiza automaticamente as abas do navegador em grupos inteligentes baseados em domínio, subdomínio ou regras personalizadas. Mantém sua sessão de navegação organizada e limpa sem esforço manual.",
  "main": "background.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tailwindcss -i ./src/input.css -o ./vendor/tailwind.css --minify",
    "watch": "tailwindcss -i ./src/input.css -o ./vendor/tailwind.css --minify --watch",
    "package": "node build.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ShadyBS/tab-group-automator.git"
  },
  "keywords": [
    "browser-extension",
    "webextension",
    "tab-management",
    "tab-grouping",
    "firefox",
    "chrome",
    "productivity",
    "automation",
    "organization",
    "tabs"
  ],
  "author": "ShadyBS",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ShadyBS/tab-group-automator/issues"
  },
  "homepage": "https://github.com/ShadyBS/tab-group-automator#readme"
}
